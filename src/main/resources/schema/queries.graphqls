type Query {
  # Query Currency
  getCurrency(baseCode: String!, targetCode: String!): Currency

  # Query CalculatedLandedCost
  calculateLandedCost(productId: UUID!, countryId: UUID!, targetCurrencyCode: String!, baseCurrencyCode: String!): LandedCost

  # Query Country
  countries: [Country]
